
@{
    ViewBag.Title = "FacebookPost";
}
<style>
    .btn-primary {
        background-color:#4267b2;
        border-color:#4267b2;
    }

    textarea {
        width:100%;
    }
</style>
<h3>Postear Contenido en Facebook</h3>

@using (Html.BeginForm())
{
    <fieldset>
        <div class="editor-field">
            <textarea name="txtMessage"></textarea>
        </div>
        <p>
            <input type="submit" value="Post" />
        </p>
    </fieldset>
}



